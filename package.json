{
  "name": "gatsby-tailwind-minimal-starter",
  "description": "Sample app built using Gatsby and Tailwind CSS",
  "version": "0.0.1",
  "private": false,
  "author": "Nirmalya Ghosh <nirmalya.email@gmail.com>",
  "keywords": [
    "gatsby",
    "tailwindcss"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "gatsby": "^2.13.50",
    "gatsby-plugin-postcss": "^2.1.2",
    "gatsby-plugin-react-helmet": "^3.1.3",
    "gatsby-plugin-typescript": "^2.1.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "tailwindcss": "^1.0.6",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/node": "^12.6.9",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-helmet": "^5.0.8",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3"
  }
}
